@Table ( "t_objectfiles" ), Versioned
segment Sparx.EA.ObjectFile 
(
    @Column,StringLength ( 255 )
    FileName : String
)
{
    @StringLength ( 50 )
    Type : String
,
    @Column()
    Note : String
,
    @StringLength ( 255 )
    FileSize : String
,
    @StringLength ( 255 )
    FileDate : String
,
    Deleted = false
};
