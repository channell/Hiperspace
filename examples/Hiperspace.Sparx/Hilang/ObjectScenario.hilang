@Table ( "t_objectscenarios" ), Versioned
segment Sparx.EA.ObjectScenario 
(
    Scenario : String
)
{
    @StringLength ( 12 )
    ScenarioType : String
,
    EValue : Double
,
    @Column()
    Notes : String
,
    @Column()
    XMLContent : String
,
    @StringLength ( 40 ), Column ( "ea_guid" )
    GUID : String
,
    Deleted = false
};
