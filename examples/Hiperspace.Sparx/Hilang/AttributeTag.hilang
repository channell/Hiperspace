@Table ( "t_attributetag" )
value Sparx.EA.AttributeTag 
(
    @KeyAttribute, Column ( "PropertyID" ), DatabaseGenerated ( DatabaseGeneratedOption.Identity )
    PropertyId : Int32
)
{
    @StringLength ( 40 ), Column ( "ea_guid" )
    GUID : String
,
    @StringLength ( 255 )
    Property : String 
,
    @StringLength ( 255 ), Column ( "VALUE" )
    VALUE : String 
,
    @Column("NOTES" )
    Notes : String 
};
