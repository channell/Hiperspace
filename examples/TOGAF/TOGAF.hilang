      Togaf.Edge2 (From = this, To = owner, Name = Name, TypeName = "AF-Requirement-For") ;

segment Togaf.Has.Location : Togaf.Base 
    = Node        ( SKey = SKey, Name = Name, TypeName = "AF-Location"),
