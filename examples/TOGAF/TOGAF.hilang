      Togaf.Edge2 (From = this, To = owner, Name = Name, TypeName = "AF-Assumption-For") ;

segment Togaf.Has.Requirement : Togaf.Base 
    = Node        ( SKey = SKey, Name = Name, TypeName = "AF-Requirement"),
